#################################################################
#    Object Properties
#################################################################

###  http://w3id.org/digitalconstruction/Variables#hasDataValue
:hasDataValue rdf:type owl:ObjectProperty ;
              rdfs:domain :Variable ;
              rdfs:range rdfs:Resource .


###  http://w3id.org/digitalconstruction/Variables#hasObjectValue
:hasObjectValue rdf:type owl:ObjectProperty ;
                rdfs:domain :Variable ;
                rdfs:range owl:Thing ;
                rdfs:comment "An object property to associate a Variable with an object value" ;
                rdfs:label "hasObjectValue" .


###  http://w3id.org/digitalconstruction/Variables#hasPropertyVariable
:hasPropertyVariable rdf:type owl:ObjectProperty ;
                     owl:inverseOf :isVariableOf ;
                     rdfs:domain owl:Thing ;
                     rdfs:range :Variable ;
                     rdfs:comment "A relation to associate a Variable to any object" ;
                     rdfs:label "hasPropertyVariable" .


###  http://w3id.org/digitalconstruction/Variables#hasVariable
:hasVariable rdf:type owl:ObjectProperty ;
             owl:inverseOf :isConstrainedBy ;
             rdfs:domain :Constraint ;
             rdfs:range :Variable ;
             rdfs:comment "A relation to associate a Constraint with a Variable" ;
             rdfs:label "hasVariable" .


###  http://w3id.org/digitalconstruction/Variables#hasVariable1
:hasVariable1 rdf:type owl:ObjectProperty ;
              rdfs:subPropertyOf :hasVariable ;
              rdfs:domain :Constraint ;
              rdfs:range :Variable ;
              rdfs:comment "The first variable of a constraint" ;
              rdfs:label "hasVariable1" .


###  http://w3id.org/digitalconstruction/Variables#hasVariable2
:hasVariable2 rdf:type owl:ObjectProperty ;
              rdfs:subPropertyOf :hasVariable ;
              rdfs:domain :Constraint ;
              rdfs:range :Variable ;
              rdfs:comment "The second variable of a constraint" ;
              rdfs:label "hasVariable2" .


###  http://w3id.org/digitalconstruction/Variables#hasVariable3
:hasVariable3 rdf:type owl:ObjectProperty ;
              rdfs:subPropertyOf :hasVariable ;
              rdfs:domain :Constraint ;
              rdfs:range :Variable ;
              rdfs:comment "The third variable of a constraint" ;
              rdfs:label "hasVariable3" .


###  http://w3id.org/digitalconstruction/Variables#hasVariableDomain
:hasVariableDomain rdf:type owl:ObjectProperty ;
                   owl:inverseOf :isVariableDomainOf ;
                   rdfs:domain :Variable ;
                   rdfs:range :VariableDomain ;
                   rdfs:comment "The relation that associates a Variable with a VariableDomain" ;
                   rdfs:label "hasVariableDomain" .


###  http://w3id.org/digitalconstruction/Variables#isConstrainedBy
:isConstrainedBy rdf:type owl:ObjectProperty ;
                 rdfs:domain :Variable ;
                 rdfs:range :Constraint ;
                 rdfs:comment "The relation that associates a Variable with a Constraint" ;
                 rdfs:label "isConstrainedBy" .


###  http://w3id.org/digitalconstruction/Variables#isVariableDomainOf
:isVariableDomainOf rdf:type owl:ObjectProperty ;
                    rdfs:domain :VariableDomain ;
                    rdfs:range :Variable ;
                    rdfs:comment "The relation that associates a VariableDomain with a Variable" ;
                    rdfs:label "isVariableDomainOf" .


###  http://w3id.org/digitalconstruction/Variables#isVariableFor
:isVariableFor rdf:type owl:ObjectProperty ;
               rdfs:domain :Variable ;
               rdfs:range rdfs:Resource ;
               rdfs:comment "The property that associates a Variable with the property whose variable it is" ;
               rdfs:label "isVariableFor" .


###  http://w3id.org/digitalconstruction/Variables#isVariableOf
:isVariableOf rdf:type owl:ObjectProperty ;
              rdfs:domain :Variable ;
              rdfs:range owl:Thing ;
              rdfs:comment "The relation that associates the Variable with the object to which the variable belongs to" ;
              rdfs:label "isVariableOf" .


#################################################################
#    Data properties
#################################################################

###  http://w3id.org/digitalconstruction/Variables#hasDataValue
:hasDataValue rdf:type owl:DatatypeProperty ;
              rdfs:domain :Variable .


#################################################################
#    Classes
#################################################################

###  http://w3id.org/digitalconstruction/Variables#AllDifferent
:AllDifferent rdf:type owl:Class ;
              rdfs:subClassOf :Constraint ;
              rdfs:comment "A constraint that states that the involved variables have different values" ;
              rdfs:label "AllDifferent" .


###  http://w3id.org/digitalconstruction/Variables#BinaryConstraint
:BinaryConstraint rdf:type owl:Class ;
                  rdfs:subClassOf :Constraint ;
                  rdfs:comment "A Constraint between two Variables" ;
                  rdfs:label "BinaryConstraint" .


###  http://w3id.org/digitalconstruction/Variables#Constraint
:Constraint rdf:type owl:Class ;
            rdfs:comment "A constraint limits the possible values of variables" ;
            rdfs:label "Constraint" .


###  http://w3id.org/digitalconstruction/Variables#ContinuousVariable
:ContinuousVariable rdf:type owl:Class ;
                    rdfs:subClassOf :Variable ;
                    rdfs:comment "A variable with continous domain of values" ;
                    rdfs:label "ContinuousVariable" .


###  http://w3id.org/digitalconstruction/Variables#DifferentValue
:DifferentValue rdf:type owl:Class ;
                rdfs:subClassOf :BinaryConstraint ;
                rdfs:comment "A BinaryConstraint that limits the values of the two variables to be different" ;
                rdfs:label "DifferentValue" .


###  http://w3id.org/digitalconstruction/Variables#DiscreteVariable
:DiscreteVariable rdf:type owl:Class ;
                  rdfs:subClassOf :Variable ;
                  rdfs:comment "A variable with a discrete domain of values" ;
                  rdfs:label "DiscreteVariable" .


###  http://w3id.org/digitalconstruction/Variables#LessThan
:LessThan rdf:type owl:Class ;
          rdfs:subClassOf :BinaryConstraint ;
          rdfs:comment "A BinaryConstraint for the value of variable1 to be less than the value of variable2" ;
          rdfs:label "LessThan" .


###  http://w3id.org/digitalconstruction/Variables#MaxValue
:MaxValue rdf:type owl:Class ;
          rdfs:subClassOf :UnaryConstraint ;
          rdfs:comment "A UnaryConstraint that limits the maximum value of the variable" ;
          rdfs:label "MaxValue" .


###  http://w3id.org/digitalconstruction/Variables#MinValue
:MinValue rdf:type owl:Class ;
          rdfs:subClassOf :UnaryConstraint ;
          rdfs:comment "A UnaryConstraint that limits the minimum value of the variable" ;
          rdfs:label "MinValue" .


###  http://w3id.org/digitalconstruction/Variables#OneOfValues
:OneOfValues rdf:type owl:Class ;
             rdfs:subClassOf :UnaryConstraint ;
             rdfs:comment "A unary constraint that limits the values of a variable to one of the enumerated values" ;
             rdfs:label "OneOfValues" .


###  http://w3id.org/digitalconstruction/Variables#SameValue
:SameValue rdf:type owl:Class ;
           rdfs:subClassOf :BinaryConstraint ;
           rdfs:comment "A BinaryConstraint for the two variables to have the same value" ;
           rdfs:label "SameValue" .


###  http://w3id.org/digitalconstruction/Variables#UnaryConstraint
:UnaryConstraint rdf:type owl:Class ;
                 rdfs:subClassOf :Constraint ;
                 rdfs:comment "A Constraint on one Variable" ;
                 rdfs:label "UnaryConstraint" .


###  http://w3id.org/digitalconstruction/Variables#Variable
:Variable rdf:type owl:Class ;
          rdfs:comment "A representation of a property that can assume different values, possibly limited by constraints" ;
          rdfs:label "Variable" .


###  http://w3id.org/digitalconstruction/Variables#VariableDomain
:VariableDomain rdf:type owl:Class ;
                rdfs:comment "An instance/literal level representation for the set of possible values that can be given to the variable" ;
                rdfs:label "VariableDomain" .


###  http://www.w3.org/2000/01/rdf-schema#Resource
rdfs:Resource rdf:type owl:Class .


#################################################################
#    Annotations
#################################################################

:hasDataValue rdfs:label "hasDataValue" ;
              rdfs:comment "The representation of a changing or as-of-yet undefined value of a property, possibly subject to constraints. Any object and RDF property can be associated with a variable." ,
                           "The data property that associates a Variable with a data value" .


#################################################################
#    General axioms
#################################################################

[ rdf:type owl:AllDisjointClasses ;
  owl:members ( :Constraint
                :Variable
                :VariableDomain
              )
] .


[ rdf:type owl:AllDisjointProperties ;
  owl:members ( :hasObjectValue
                :hasPropertyVariable
                :hasVariable
                :hasVariableDomain
              )
] .


