        <rdfs:label>hasVariableDomain</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- https://w3id.org/digitalconstruction/Variables#isApplicableIn -->


    <owl:ObjectProperty rdf:about="https://w3id.org/digitalconstruction/Variables#isApplicableIn">
